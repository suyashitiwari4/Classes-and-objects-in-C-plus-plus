#include<iostream>
using namespace std;

    class Car {
        public:
        string brand;
        string model;
        string year;
        
        void info(){
            
            cout<<"Enter the brand of car: "<<endl;
            cin>>brand;
            cout<<"Enter the model of car:"<<endl;
            cin>>model;
            cout<<"Enter the year of car:"<<endl;
            cin>>year;
            
        
        }
    };
int main(){
    Car c1;
    c1.info();
    cout<<"Brand:"<<c1.brand<<endl;;
    cout<<"Model"<<c1.model<<endl;
    cout<<"Year:"<<c1.year<<endl;
  
  
    
    return 0;
}
/*OUTPUT:
Enter the brand of car: 
MARUTI
Enter the model of car:
DZIRE
Enter the year of car:
2000
Brand:MARUTI
ModelDZIRE
Year:2000


=== Code Execution Successful ===*/
